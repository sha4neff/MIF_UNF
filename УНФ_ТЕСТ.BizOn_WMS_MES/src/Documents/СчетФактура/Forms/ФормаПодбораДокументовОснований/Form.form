	ЗаказНаряды.СуммаДокумента,
	ЛОЖЬ,
	ЗНАЧЕНИЕ(Перечисление.ДаНет.Нет)
ИЗ
	Документ.ЗаказПокупателя КАК ЗаказНаряды
ГДЕ
	ЗаказНаряды.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийЗаказПокупателя.ЗаказНаряд)
	И &amp;РеализацииДоступны = ИСТИНА

ОБЪЕДИНИТЬ

ВЫБРАТЬ
	Переработка.Ссылка,
	Переработка.Дата,
	Переработка.Организация,
	Переработка.Контрагент,
	Переработка.Договор,
	Переработка.СуммаДокумента,
	ЛОЖЬ,
	ЗНАЧЕНИЕ(Перечисление.ДаНет.Нет)
ИЗ
	Документ.ОтчетОПереработке КАК Переработка
ГДЕ
	&amp;РеализацииДоступны = ИСТИНА

ОБЪЕДИНИТЬ

ВЫБРАТЬ
	Ремонт.Ссылка,
	Ремонт.Дата,
	Ремонт.Организация,
	Ремонт.Контрагент,
	Ремонт.Договор,
	Ремонт.СуммаДокумента,
	ЛОЖЬ,
	ЗНАЧЕНИЕ(Перечисление.ДаНет.Нет)
ИЗ
	Документ.ПриемИПередачаВРемонт КАК Ремонт
ГДЕ
	&amp;РеализацииДоступны = ИСТИНА

ОБЪЕДИНИТЬ

ВЫБРАТЬ
	Накладная.Ссылка,
	Накладная.Дата,
	Накладная.Организация,
	Накладная.Контрагент,
	Накладная.Договор,
	Накладная.СуммаДокумента,
	ЛОЖЬ,
	ВЫБОР
		КОГДА Накладная.ВидОперации В (ЗНАЧЕНИЕ(Перечисление.ВидыОперацийРасходнаяНакладная.ВозвратПоставщику), ЗНАЧЕНИЕ(Перечисление.ВидыОперацийРасходнаяНакладная.ВозвратКомитенту), ЗНАЧЕНИЕ(Перечисление.ВидыОперацийРасходнаяНакладная.ВозвратИзПереработки), ЗНАЧЕНИЕ(Перечисление.ВидыОперацийРасходнаяНакладная.ВозвратСОтветхранения))
			ТОГДА ЗНАЧЕНИЕ(Перечисление.ДаНет.Да)
		ИНАЧЕ ЗНАЧЕНИЕ(Перечисление.ДаНет.Нет)
	КОНЕЦ
ИЗ
	Документ.РасходнаяНакладная КАК Накладная
ГДЕ
	&amp;РеализацииДоступны = ИСТИНА

ОБЪЕДИНИТЬ

ВЫБРАТЬ
	ОтчетКомитенту.Ссылка,
	ОтчетКомитенту.Дата,
	ОтчетКомитенту.Организация,
	ОтчетКомитенту.Контрагент,
	ОтчетКомитенту.Договор,
	0,
	ЛОЖЬ,
	ЗНАЧЕНИЕ(Перечисление.ДаНет.Нет)
ИЗ
	Документ.ОтчетКомитенту КАК ОтчетКомитенту
ГДЕ
	&amp;ОтчетКомитентуДоступен = ИСТИНА

ОБЪЕДИНИТЬ

ВЫБРАТЬ
	ОперацияПоПлатежнымКартам.Ссылка,
	ОперацияПоПлатежнымКартам.Дата,
	ОперацияПоПлатежнымКартам.Организация,
	ОперацияПоПлатежнымКартам.Контрагент,
	НЕОПРЕДЕЛЕНО,
	ОперацияПоПлатежнымКартам.СуммаДокумента,
	ЛОЖЬ,
	ЗНАЧЕНИЕ(Перечисление.ДаНет.Нет)
ИЗ
	Документ.ОперацияПоПлатежнымКартам КАК ОперацияПоПлатежнымКартам
ГДЕ
	&amp;АвансыДоступны = ИСТИНА

ОБЪЕДИНИТЬ

ВЫБРАТЬ
	ПоступлениеНаСчет.Ссылка,
	ПоступлениеНаСчет.Дата,
	ПоступлениеНаСчет.Организация,
	ПоступлениеНаСчет.Контрагент,
	НЕОПРЕДЕЛЕНО,
	ПоступлениеНаСчет.СуммаДокумента,
	ЛОЖЬ,
	ЗНАЧЕНИЕ(Перечисление.ДаНет.Нет)
ИЗ
	Документ.ПоступлениеНаСчет КАК ПоступлениеНаСчет
ГДЕ
	&amp;АвансыДоступны = ИСТИНА

ОБЪЕДИНИТЬ

ВЫБРАТЬ
	ПоступлениеВКассу.Ссылка,
	ПоступлениеВКассу.Дата,
	ПоступлениеВКассу.Организация,
	ПоступлениеВКассу.Контрагент,
	НЕОПРЕДЕЛЕНО,
	ПоступлениеВКассу.СуммаДокумента,
	ЛОЖЬ,
	ЗНАЧЕНИЕ(Перечисление.ДаНет.Нет)
ИЗ
	Документ.ПоступлениеВКассу КАК ПоступлениеВКассу
ГДЕ
	&amp;АвансыДоступны = ИСТИНА

ОБЪЕДИНИТЬ

ВЫБРАТЬ
	Корректировка.Ссылка,
	Корректировка.ДатаВходящегоДокумента,
	Корректировка.Организация,
	Корректировка.Контрагент,
	Корректировка.Договор,
	Корректировка.СуммаДокумента,
	ЛОЖЬ,
	ЗНАЧЕНИЕ(Перечисление.ДаНет.Нет)
ИЗ
	Документ.КорректировкаРеализации КАК Корректировка</queryText>
