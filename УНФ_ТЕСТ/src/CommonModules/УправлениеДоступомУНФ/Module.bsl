// Обновляет доступ служебного пользователя к API.
//
// Параметры:
//  Пользователь - СправочникСсылка.Пользователи - служебный пользователь
//  Включить	 - Булево - установить или снять роли, в зависимости от признака
//
Процедура ОбновитьДоступСлужебногоПользователяКAPI(Пользователь, Включить) Экспорт
	
	Роли = РолиСлужебногоПользователяИнтеграции();
	
	Если Включить Тогда
		УстановитьРолиСлужебногоПользователя(Пользователь, Роли);
	Иначе
		СнятьРолиСлужебногоПользователя(Пользователь, Роли);
	КонецЕсли;
	
КонецПроцедуры

#КонецОбласти

#Область СлужебныеПроцедурыИФункции

Функция НовыеСвойстваПрофиля(Имя, Идентификатор)
	
	Результат = Новый Структура;
	Результат.Вставить("Имя", Имя);
	Результат.Вставить("Идентификатор", Идентификатор);
	Результат.Вставить("УИД", Новый УникальныйИдентификатор(Идентификатор));
	
	Возврат Результат;
	
КонецФункции

Функция РолиСлужебногоПользователяИнтеграции()
	
	Роли = Новый Массив;
	Роли.Добавить(Метаданные.Роли.УдаленныйДоступПередачаДанных.Имя);
	Роли.Добавить(Метаданные.Роли.УдалениеДанныхOData.Имя);
	
	Возврат Роли;
	
КонецФункции

Процедура УстановитьРолиСлужебногоПользователя(Пользователь, Роли)
	
	СвойстваПользователя = СвойстваПользователя(Пользователь);
	Если СвойстваПользователя = Неопределено Тогда
		ЗаписьЖурналаРегистрации(ИмяСобытияЖурналаРегистрации(),
			УровеньЖурналаРегистрации.Ошибка, Метаданные.Справочники.Пользователи, Пользователь, 
			СтрШаблон(НСтр("ru = 'Для %1 не найден пользователь информационной базы'"), Пользователь));
		Возврат;
	КонецЕсли;
	
	ОбщегоНазначенияКлиентСервер.ДополнитьМассив(СвойстваПользователя.Роли, Роли, Истина);
	СвойстваДляЗаписи = Новый Структура("Роли", СвойстваПользователя.Роли);
	Пользователи.УстановитьСвойстваПользователяИБ(СвойстваПользователя.УникальныйИдентификатор, СвойстваДляЗаписи);
	
КонецПроцедуры

Процедура СнятьРолиСлужебногоПользователя(Пользователь, Роли)
	
	СвойстваПользователя = СвойстваПользователя(Пользователь);
	Если СвойстваПользователя = Неопределено Тогда
		ЗаписьЖурналаРегистрации(ИмяСобытияЖурналаРегистрации(),
			УровеньЖурналаРегистрации.Ошибка, Метаданные.Справочники.Пользователи, Пользователь, 
			СтрШаблон(НСтр("ru = 'Для %1 не найден пользователь информационной базы'"), Пользователь));
		Возврат;
	КонецЕсли;
	
	РолиПользователя = ОбщегоНазначенияКлиентСервер.РазностьМассивов(СвойстваПользователя.Роли, Роли);
	СвойстваДляЗаписи = Новый Структура("Роли", РолиПользователя);
	Пользователи.УстановитьСвойстваПользователяИБ(СвойстваПользователя.УникальныйИдентификатор, СвойстваДляЗаписи);
	
КонецПроцедуры

Функция СвойстваПользователя(Пользователь)
	
	Если НЕ ЗначениеЗаполнено(Пользователь) Тогда
		Возврат Неопределено;
	КонецЕсли;
	
	ИдентификаторПользователяИБ = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(Пользователь, "ИдентификаторПользователяИБ");
	Если НЕ ЗначениеЗаполнено(ИдентификаторПользователяИБ) Тогда
		Возврат Неопределено;
	КонецЕсли;
	
	Возврат Пользователи.СвойстваПользователяИБ(ИдентификаторПользователяИБ);
	
КонецФункции

Функция ИмяСобытияЖурналаРегистрации()
	
	Возврат НСтр("ru = 'Управление доступом УНФ.Обновить доступ служебного пользователя к API'",
		ОбщегоНазначения.КодОсновногоЯзыка());
	
КонецФункции

#КонецОбласти
