//
Процедура ПриЗаполненииВидовДоступа(ВидыДоступа) Экспорт
	
	ВидДоступа = ВидыДоступа.Добавить();
	ВидДоступа.Имя = "Кассы";
	ВидДоступа.Представление    = НСтр("ru = 'Кассы'");
	ВидДоступа.ТипЗначений      = Тип("СправочникСсылка.Кассы");
	
	ВидДоступа = ВидыДоступа.Добавить();
	ВидДоступа.Имя = "ГруппыКонтрагентов";
	ВидДоступа.Представление    = НСтр("ru = 'Группы контактов и контрагентов'");
	ВидДоступа.ТипЗначений      = Тип("СправочникСсылка.Контрагенты");
	ВидДоступа.ТипГруппЗначений = Тип("СправочникСсылка.ГруппыДоступаКонтрагентов");
	УправлениеДоступом.ДобавитьДополнительныеТипыВидаДоступа(ВидДоступа,Тип("СправочникСсылка.КонтактныеЛица"),ВидДоступа.ТипГруппЗначений,);
	
	ВидДоступа = ВидыДоступа.Добавить();
	ВидДоступа.Имя = "ГруппыЛидов";
	ВидДоступа.Представление    = НСтр("ru = 'Группы лидов'");
	ВидДоступа.ТипЗначений      = Тип("СправочникСсылка.Лиды");
	ВидДоступа.ТипГруппЗначений = Тип("СправочникСсылка.ГруппыДоступаЛидов");
	
	ВидДоступа = ВидыДоступа.Добавить();
	ВидДоступа.Имя = "ВидыДоговоровКредитаИЗайма";
	ВидДоступа.Представление    = НСтр("ru = 'Виды договоров кредита и займа'");
	ВидДоступа.ТипЗначений      = Тип("ПеречислениеСсылка.ВидыДоговоровКредитаИЗайма");
	
	ВидДоступа = ВидыДоступа.Добавить();
	ВидДоступа.Имя = "Организации";
	ВидДоступа.Представление = НСтр("ru = 'Организации'");
	ВидДоступа.ТипЗначений   = Тип("СправочникСсылка.Организации");
	
	ВидДоступа = ВидыДоступа.Добавить();
	ВидДоступа.Имя = "БанковскиеСчета";
	ВидДоступа.Представление = НСтр("ru = 'Банковские счета'");
	ВидДоступа.ТипЗначений   = Тип("СправочникСсылка.БанковскиеСчета");
	
	ВидДоступа = ВидыДоступа.Добавить();
	ВидДоступа.Имя = "КассыККМ";
	ВидДоступа.Представление = НСтр("ru = 'Кассы ККМ'");
	ВидДоступа.ТипЗначений   = Тип("СправочникСсылка.КассыККМ");
	
	ВидДоступа = ВидыДоступа.Добавить();
	ВидДоступа.Имя = "ВидыЦен";
	ВидДоступа.Представление = НСтр("ru = 'Виды цен'");
	ВидДоступа.ТипЗначений   = Тип("СправочникСсылка.ВидыЦен");
	
