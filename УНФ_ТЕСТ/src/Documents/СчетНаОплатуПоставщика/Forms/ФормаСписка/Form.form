    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	ВЫБОР
		КОГДА ЕСТЬNULL(ФактОплаты.СуммаОплаты, 0) + ЕСТЬNULL(ФактОплаты.СуммаАванса, 0) = 0
			ТОГДА ВЫБОР
					КОГДА ДокументСчетНаОплату.ЗапланироватьОплату
							И НЕ ГрафикОплаты.СчетНаОплату ЕСТЬ NULL
						ТОГДА ВЫБОР
								КОГДА ГрафикОплаты.Период &lt; &amp;АкутальнаяДатаСеанса
									ТОГДА 6
								КОГДА ГрафикОплаты.Период = &amp;АкутальнаяДатаСеанса
									ТОГДА 4
								ИНАЧЕ 2
							КОНЕЦ
					ИНАЧЕ 2
				КОНЕЦ
		КОГДА ЕСТЬNULL(ФактОплаты.Сумма, 0) = ЕСТЬNULL(ФактОплаты.СуммаОплаты, 0) + ЕСТЬNULL(ФактОплаты.СуммаАванса, 0)
			ТОГДА 0
		КОГДА ЕСТЬNULL(ФактОплаты.Сумма, 0) &lt; ЕСТЬNULL(ФактОплаты.СуммаОплаты, 0) + ЕСТЬNULL(ФактОплаты.СуммаАванса, 0)
			ТОГДА 7
		ИНАЧЕ ВЫБОР
				КОГДА ДокументСчетНаОплату.ЗапланироватьОплату
						И НЕ ГрафикОплаты.СчетНаОплату ЕСТЬ NULL
					ТОГДА ВЫБОР
							КОГДА ГрафикОплаты.Период &lt; &amp;АкутальнаяДатаСеанса
								ТОГДА 5
							КОГДА ГрафикОплаты.Период = &amp;АкутальнаяДатаСеанса
								ТОГДА 3
							ИНАЧЕ 1
						КОНЕЦ
				ИНАЧЕ 1
			КОНЕЦ
	КОНЕЦ КАК НомерКартинкиОплаты,
	ДокументСчетНаОплату.Ссылка КАК Ссылка,
	ДокументСчетНаОплату.ПометкаУдаления КАК ПометкаУдаления,
	ДокументСчетНаОплату.Номер КАК Номер,
	ДокументСчетНаОплату.Дата КАК Дата,
	ДокументСчетНаОплату.Проведен КАК Проведен,
	ДокументСчетНаОплату.Организация КАК Организация,
	ДокументСчетНаОплату.Комментарий КАК Комментарий,
	ДокументСчетНаОплату.ДокументОснование КАК ДокументОснование,
	ДокументСчетНаОплату.Контрагент КАК Контрагент,
	ДокументСчетНаОплату.Контрагент.КонтактноеЛицо КАК КонтактноеЛицо,
	ДокументСчетНаОплату.Договор КАК Договор,
	ДокументСчетНаОплату.ТипДенежныхСредств КАК ТипДенежныхСредств,
	ДокументСчетНаОплату.БанковскийСчет КАК БанковскийСчет,
	ДокументСчетНаОплату.ВалютаДокумента КАК ВалютаДокумента,
	ДокументСчетНаОплату.Кратность КАК Кратность,
	ДокументСчетНаОплату.Курс КАК Курс,
	ДокументСчетНаОплату.СуммаВключаетНДС КАК СуммаВключаетНДС,
	ДокументСчетНаОплату.ВидЦенКонтрагента КАК ВидЦенКонтрагента,
	ДокументСчетНаОплату.СуммаДокумента КАК СуммаДокумента,
	ДокументСчетНаОплату.Касса КАК Касса,
	ДокументСчетНаОплату.Ответственный КАК Ответственный,
	ДокументСчетНаОплату.МоментВремени КАК МоментВремени,
	ДокументСчетНаОплату.Автор КАК Автор,
	ДокументСчетНаОплату.НомерВходящегоДокумента КАК НомерВходящегоДокумента,
	ДокументСчетНаОплату.ДатаВходящегоДокумента КАК ДатаВходящегоДокумента,
	ВЫБОР
		КОГДА ДокументСчетНаОплату.Проведен
				И ДокументСчетНаОплату.СуммаДокумента > 0
			ТОГДА ВЫРАЗИТЬ((ЕСТЬNULL(ФактОплаты.СуммаОплаты, 0) + ЕСТЬNULL(ФактОплаты.СуммаАванса, 0)) * 100 / ДокументСчетНаОплату.СуммаДокумента КАК ЧИСЛО(15, 2))
		ИНАЧЕ 0
	КОНЕЦ КАК ПроцентОплаты,
	ВЫБОР
		КОГДА ДокументСчетНаОплату.Проведен
				И ДокументСчетНаОплату.СуммаДокумента > 0
			ТОГДА ВЫБОР
					КОГДА ЕСТЬNULL(ФактОплаты.СуммаОплаты, 0) + ЕСТЬNULL(ФактОплаты.СуммаАванса, 0) - ДокументСчетНаОплату.СуммаДокумента >= 0
						ТОГДА &quot;Оплачен полностью&quot;
					КОГДА ЕСТЬNULL(ФактОплаты.СуммаОплаты, 0) + ЕСТЬNULL(ФактОплаты.СуммаАванса, 0) > 0
						ТОГДА &quot;Оплачен частично&quot;
					ИНАЧЕ &quot;Без оплаты&quot;
				КОНЕЦ
		ИНАЧЕ &quot;Без оплаты&quot;
	КОНЕЦ КАК СтатусОплаты,
	ЕСТЬNULL(ФактОплаты.СуммаОплаты, 0) + ЕСТЬNULL(ФактОплаты.СуммаАванса, 0) КАК СуммаОплатыОборот,
	ДокументСчетНаОплату.Подразделение КАК Подразделение,
	СостоянияЭД.СостояниеВерсииЭД КАК СостояниеВерсииЭД,
	ВЫБОР
		КОГДА НаличиеФайлов.ЕстьФайлы ЕСТЬ NULL
			ТОГДА 0
		КОГДА НаличиеФайлов.ЕстьФайлы
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ КАК ЕстьФайлы,
	NULL КАК СостояниеОригиналаПервичногоДокумента,
	ИСТИНА КАК ОбщееСостояние,
	0 КАК СостояниеОригиналПолучен,
	ЕСТЬNULL(СостоянияОригиналовПервичныхДокументов.Состояние,
		ЗНАЧЕНИЕ(Справочник.СостоянияОригиналовПервичныхДокументов.ПустаяСсылка)) КАК СостояниеОригинала
ИЗ
	Документ.СчетНаОплатуПоставщика КАК ДокументСчетНаОплату
		{ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ФактОплатыЗаказов КАК ФактОплаты
		ПО ДокументСчетНаОплату.Ссылка = ФактОплаты.СчетНаОплату}
		{ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ГрафикОплатыЗаказов КАК ГрафикОплаты
		ПО ДокументСчетНаОплату.Ссылка = ГрафикОплаты.СчетНаОплату
			И (ГрафикОплаты.Период &lt;= &amp;АкутальнаяДатаСеанса)}
		{ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СостоянияЭД КАК СостоянияЭД
		ПО ДокументСчетНаОплату.Ссылка = СостоянияЭД.СсылкаНаОбъект}
		{ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.НаличиеФайлов КАК НаличиеФайлов
		ПО ДокументСчетНаОплату.Ссылка = НаличиеФайлов.ОбъектСФайлами}
		{ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СостоянияОригиналовПервичныхДокументов КАК СостоянияОригиналовПервичныхДокументов
		ПО ДокументСчетНаОплату.Ссылка = СостоянияОригиналовПервичныхДокументов.Ссылка
		И СостоянияОригиналовПервичныхДокументов.ОбщееСостояние = Истина}</queryText>
