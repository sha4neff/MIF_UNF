    <name>Список</name>
    <id>1</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>Список.Контрагент</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>Список.Ссылка</segments>
    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	ВЫБОР
		КОГДА ДокументДополнительныеРасходы.СуммаДокумента > 0
			ТОГДА ВЫБОР
					КОГДА НЕ ДокументДополнительныеРасходы.Контрагент.ВестиРасчетыПоДокументам
							ИЛИ ФактОплаты.Сумма ЕСТЬ NULL
						ТОГДА -1
					КОГДА ЕСТЬNULL(ФактОплаты.СуммаОплаты, 0) + ЕСТЬNULL(ФактОплаты.СуммаАванса, 0) = 0
						ТОГДА 2
					КОГДА ЕСТЬNULL(ФактОплаты.СуммаОплаты, 0) + ЕСТЬNULL(ФактОплаты.СуммаАванса, 0) = ДокументДополнительныеРасходы.СуммаДокумента
						ТОГДА 0
					КОГДА ЕСТЬNULL(ФактОплаты.СуммаОплаты, 0) + ЕСТЬNULL(ФактОплаты.СуммаАванса, 0) > ДокументДополнительныеРасходы.СуммаДокумента
						ТОГДА 7
					ИНАЧЕ 1
				КОНЕЦ
		ИНАЧЕ -1
	КОНЕЦ КАК НомерКартинкиОплаты,
	ВЫБОР
		КОГДА ДокументДополнительныеРасходы.СуммаДокумента > 0
			ТОГДА ВЫБОР
					КОГДА ФактОплаты.Сумма ЕСТЬ NULL
						ТОГДА &quot;Не учитывается&quot;
					КОГДА ЕСТЬNULL(ФактОплаты.СуммаОплаты, 0) + ЕСТЬNULL(ФактОплаты.СуммаАванса, 0) - ДокументДополнительныеРасходы.СуммаДокумента >= 0
						ТОГДА &quot;Оплачен полностью&quot;
					КОГДА ЕСТЬNULL(ФактОплаты.СуммаОплаты, 0) + ЕСТЬNULL(ФактОплаты.СуммаАванса, 0) > 0
						ТОГДА &quot;Оплачен частично&quot;
					ИНАЧЕ &quot;Без оплаты&quot;
				КОНЕЦ
		ИНАЧЕ &quot;Не учитывается&quot;
	КОНЕЦ КАК СтатусОплаты,
	ЕСТЬNULL(ФактОплаты.Сумма, 0) - ЕСТЬNULL(ФактОплаты.СуммаОплаты, 0) - ЕСТЬNULL(ФактОплаты.СуммаАванса, 0) КАК КОплате,
	ВЫБОР
		КОГДА ДокументДополнительныеРасходы.Проведен
				И ДокументДополнительныеРасходы.СуммаДокумента > 0
			ТОГДА ВЫРАЗИТЬ((ЕСТЬNULL(ФактОплаты.СуммаОплаты, 0) + ЕСТЬNULL(ФактОплаты.СуммаАванса, 0)) * 100 / ДокументДополнительныеРасходы.СуммаДокумента КАК ЧИСЛО(15, 2))
		ИНАЧЕ 0
	КОНЕЦ КАК ПроцентОплаты,
	ДокументДополнительныеРасходы.Ссылка КАК Ссылка,
	ДокументДополнительныеРасходы.ПометкаУдаления КАК ПометкаУдаления,
	ДокументДополнительныеРасходы.Номер КАК Номер,
	ДокументДополнительныеРасходы.Дата КАК Дата,
	ДокументДополнительныеРасходы.Проведен КАК Проведен,
	ДокументДополнительныеРасходы.Организация КАК Организация,
	ДокументДополнительныеРасходы.Контрагент КАК Контрагент,
	ДокументДополнительныеРасходы.Договор КАК Договор,
	ДокументДополнительныеРасходы.ЗаказПоставщику КАК ЗаказПоставщику,
	ДокументДополнительныеРасходы.СтруктурнаяЕдиница КАК СтруктурнаяЕдиница,
	ДокументДополнительныеРасходы.ВалютаДокумента КАК ВалютаДокумента,
	ДокументДополнительныеРасходы.НалогообложениеНДС КАК НалогообложениеНДС,
	ДокументДополнительныеРасходы.СуммаВключаетНДС КАК СуммаВключаетНДС,
	ДокументДополнительныеРасходы.НДСВключатьВСтоимость КАК НДСВключатьВСтоимость,
	ДокументДополнительныеРасходы.Курс КАК Курс,
	ДокументДополнительныеРасходы.Кратность КАК Кратность,
	ДокументДополнительныеРасходы.Комментарий КАК Комментарий,
	ДокументДополнительныеРасходы.СуммаДокумента КАК СуммаДокумента,
	ДокументДополнительныеРасходы.Подразделение КАК Подразделение,
	ДокументДополнительныеРасходы.Ответственный КАК Ответственный,
	ДокументДополнительныеРасходы.Автор КАК Автор,
	ДокументДополнительныеРасходы.НомерВходящегоДокумента КАК НомерВходящегоДокумента,
	ДокументДополнительныеРасходы.ДатаВходящегоДокумента КАК ДатаВходящегоДокумента,
	ДокументДополнительныеРасходы.ХозяйственнаяОперация КАК ХозяйственнаяОперация,
	ДокументДополнительныеРасходы.Запасы.(
		Ссылка КАК Ссылка,
		НомерСтроки КАК НомерСтроки,
		ДокументПоступления КАК ДокументПоступления,
		Номенклатура КАК Номенклатура,
		Характеристика КАК Характеристика,
		Партия КАК Партия,
		Количество КАК Количество,
		ЕдиницаИзмерения КАК ЕдиницаИзмерения,
		Цена КАК Цена,
		Сумма КАК Сумма,
		СтавкаНДС КАК СтавкаНДС,
		СуммаНДС КАК СуммаНДС,
		Всего КАК Всего,
		ЗаказПокупателя КАК ЗаказПокупателя,
		СуммаРасходов КАК СуммаРасходов,
		Коэффициент КАК Коэффициент
	) КАК Запасы,
	ДокументДополнительныеРасходы.Расходы.(
		Ссылка КАК Ссылка,
		НомерСтроки КАК НомерСтроки,
		Номенклатура КАК Номенклатура,
		Количество КАК Количество,
		ЕдиницаИзмерения КАК ЕдиницаИзмерения,
		Цена КАК Цена,
		Сумма КАК Сумма,
		СтавкаНДС КАК СтавкаНДС,
		СуммаНДС КАК СуммаНДС,
		Всего КАК Всего
	) КАК Расходы,
	ДокументДополнительныеРасходы.Предоплата.(
		Ссылка КАК Ссылка,
		НомерСтроки КАК НомерСтроки,
		Документ КАК Документ,
		СуммаРасчетов КАК СуммаРасчетов,
		Курс КАК Курс,
		Кратность КАК Кратность,
		СуммаПлатежа КАК СуммаПлатежа
	) КАК Предоплата,
	ДокументДополнительныеРасходы.МоментВремени КАК МоментВремени,
	ВЫБОР
		КОГДА НаличиеФайлов.ЕстьФайлы ЕСТЬ NULL
			ТОГДА 0
		КОГДА НаличиеФайлов.ЕстьФайлы
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ КАК ЕстьФайлы
ИЗ
	Документ.ДополнительныеРасходы КАК ДокументДополнительныеРасходы
		{ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ФактОплатыДокументов КАК ФактОплаты
		ПО ДокументДополнительныеРасходы.Ссылка = ФактОплаты.Документ}
		{ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.НаличиеФайлов КАК НаличиеФайлов
		ПО ДокументДополнительныеРасходы.Ссылка = НаличиеФайлов.ОбъектСФайлами}</queryText>
