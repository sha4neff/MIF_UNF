    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>СписокАвтоСкидок.РаскрашиватьЖелтым</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>СписокАвтоСкидок.Ссылка</segments>
