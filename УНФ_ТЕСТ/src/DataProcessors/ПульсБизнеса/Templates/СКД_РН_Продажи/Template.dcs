		<field xsi:type="DataSetFieldField">
			<dataPath>СвойствоНоменклатуры</dataPath>
			<field>СвойствоНоменклатуры</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Свойство номенклатуры</v8:content>
