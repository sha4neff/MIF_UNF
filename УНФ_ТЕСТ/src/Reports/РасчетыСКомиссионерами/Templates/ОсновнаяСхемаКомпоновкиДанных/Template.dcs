	0,
	РасчетыСПокупателямиОбороты.Регистратор,
	РасчетыСПокупателямиОбороты.ПериодСекунда
ИЗ
	РегистрНакопления.РасчетыСПокупателями.Обороты(
			,
			,
			Авто,
			(Организация, Контрагент, Договор) В
				(ВЫБРАТЬ
					Вт_ТоварыНаКомиссии.Организация,
					Вт_ТоварыНаКомиссии.Контрагент,
					Вт_ТоварыНаКомиссии.Договор
				ИЗ
					Вт_ТоварыНаКомиссии КАК Вт_ТоварыНаКомиссии)) КАК РасчетыСПокупателямиОбороты
ГДЕ
	(ТИПЗНАЧЕНИЯ(РасчетыСПокупателямиОбороты.Регистратор) = ТИП(Документ.ПоступлениеНаСчет)
			ИЛИ ТИПЗНАЧЕНИЯ(РасчетыСПокупателямиОбороты.Регистратор) = ТИП(Документ.ПоступлениеВКассу)
			ИЛИ ТИПЗНАЧЕНИЯ(РасчетыСПокупателямиОбороты.Регистратор) = ТИП(Документ.РасходСоСчета)
			ИЛИ ТИПЗНАЧЕНИЯ(РасчетыСПокупателямиОбороты.Регистратор) = ТИП(Документ.РасходИзКассы))
;

