	(ТИПЗНАЧЕНИЯ(РасчетыСПоставщикамиОбороты.Регистратор) = ТИП(Документ.ПоступлениеНаСчет)
			ИЛИ ТИПЗНАЧЕНИЯ(РасчетыСПоставщикамиОбороты.Регистратор) = ТИП(Документ.ПоступлениеВКассу)
			ИЛИ ТИПЗНАЧЕНИЯ(РасчетыСПоставщикамиОбороты.Регистратор) = ТИП(Документ.РасходСоСчета)
			ИЛИ ТИПЗНАЧЕНИЯ(РасчетыСПоставщикамиОбороты.Регистратор) = ТИП(Документ.РасходИзКассы))
;

